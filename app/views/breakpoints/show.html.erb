          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">Master group include</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.master_group_include %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">Organism group include</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.organism_group_include %>
            </span>
          </div>
        </div>

