<% @title = "Showing breakpoint" %>

<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
      <div class="form-horizontal">
                <div class="form-group">
          <label class="col-md-2 form-label">Drug</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.drug %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">S maximum</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.s_maximum %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">R minimum</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.r_minimum %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">Surrogate drugs</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.surrogate_drugs %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">R if surrogate is</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.r_if_surrogate_is %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">Ni if surrogate is</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.ni_if_surrogate_is %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">R if blt is</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.r_if_blt_is %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">Delivery mechanism</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @breakpoint.delivery_mechanism %>
            </span>
          </div>
        </div>

                <div class="form-group">
